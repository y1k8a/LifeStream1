	<check_box label="不知道名字" name="omit_abuser_name" tool_tip="如果你無法提供實施不良行為的居民的名字，請選擇這項。"/>
	<text name="abuser_name_title2">
		不良行為發生的地址:
	</text>
	<text name="sum_title">
		總結:
	</text>
	<text name="dscr_title">
		詳細:
	</text>
