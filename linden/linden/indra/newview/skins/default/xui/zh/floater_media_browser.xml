<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
	 min_height="140"
	 min_width="467"
	 name="floater_about"
	 title="媒體流覽器">
	<layout_stack
		 name="stack1"
		 top="-20">
		<layout_panel
			 name="nav_controls">
			<button
				 label="後退"
				 name="back" />
			<button
				 label="前進"
				 name="forward" />
			<button
				 label="刷新"
				 name="reload" />
			<button
				 label="轉到"
				 name="go" />
		</layout_panel>
		<layout_panel
			 name="parcel_owner_controls">
			<button
				 label="當前網址設置為地塊媒體位址"
				 name="assign" />
		</layout_panel>
		<layout_panel
			 name="external_controls">
			<button
				 label="在我的網頁流覽器中打開"
				 name="open_browser" />
			<check_box
				 label="永遠都在我的流覽器中打開"
				 name="open_always" />
			<button
				 label="關閉"
				 name="close" />
		</layout_panel>
	</layout_stack>
</floater>

<!--Translated for Second Life 1.19.1 RC3 -->
