<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
	 name="MemLeak"
	 title="記憶體洩漏類比">
	<spinner
		 label="洩漏速度 (位元組每秒):"
		 name="leak_speed" />
	<spinner
		 label="最大洩漏記憶體 (MB):"
		 name="max_leak" />
	<text
		 name="total_leaked_label">
		當前洩漏的記憶體: [SIZE] KB
	</text>	
	<text
		 name="note_label_1">
		[NOTE1]
	</text>
	<text
		 name="note_label_2">
		[NOTE2]
	</text>	
	<button
		 label="開始"
		 name="start_btn" />
	<button
		 label="結束"
		 name="stop_btn" />
	<button
		 label="釋放"
		 name="release_btn" />
	<button
		 label="關閉"
		 name="close_btn" />
</floater>

