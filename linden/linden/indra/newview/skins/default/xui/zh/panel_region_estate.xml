<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel label="地產" name="Estate">
	<text name="estate_help_text">
		對該選項卡中設定的更改會影響到
地產內所有的區域。
	</text>
	<text name="estate_text">
		地產:
	</text>
	<text name="estate_name">
		(未知)
	</text>
	<text name="owner_text">
		主人:
	</text>
	<text name="estate_owner">
		(未知)
	</text>
	<text name="estate_manager_label">
		經理人:
	</text>
	<button label="增加..." name="add_estate_manager_btn" />
	<button label="移除..." name="remove_estate_manager_btn" />
	<check_box label="使用國際時間" name="use_global_time_check" />
	<check_box label="太陽固定" name="fixed_sun_check" />
	<slider label="相位" name="sun_hour_slider" />
	<check_box label="從大陸可視" name="externally_visible_check" />
	<check_box label="從此處可以看見大陸" name="mainland_visible_check" />
<text
		 name="Only Allow">
		限制訪問到:
	</text>
	<check_box
		 label="付費居民"
		 name="limit_payment"
		 tool_tip="封禁沒有驗證的居民。" />
	<check_box
		 label="通過年齡驗證的成年人"
		 name="limit_age_verified"
		 tool_tip="封禁沒有驗證年齡的居民。詳細資訊請參見 support.secondlife.com 。" />
	<check_box
		 label="允許語音聊天"
		 name="voice_chat_check" />
	<text
		 name="abuse_email_text">
		不良行為報告電子郵寄地址:
	</text>
	<string
		 name="email_unsupported">不支援的功能</string>

	<check_box label="允許直接瞬間移動" name="allow_direct_teleport" />
	<text name="region_text_lbl">
		拒絕通往支付情況資訊:
	</text>
	<check_box label="否認檔中的沒有完成支付的資訊" name="deny_anonymous" />
	<check_box label="否認檔中的支付資訊" name="deny_identified" />
	<check_box label="否認使用過的支付資訊" name="deny_transacted" />
	<button label="應用" name="apply_btn" />
	<text name="allow_resident_label">
		允許的居民:
	</text>
	<button label="?" name="allow_resident_help" />
	<button label="增加..." name="add_allowed_avatar_btn" />
	<button label="移除..." name="remove_allowed_avatar_btn" />
	<text name="allow_group_label">
		允許的社群:
	</text>
	<button label="?" name="allow_group_help" />
	<button label="增加..." name="add_allowed_group_btn" />
	<button label="移除..." name="remove_allowed_group_btn" />
	<text name="ban_resident_label">
		封禁的居民:
	</text>
	<button label="?" name="ban_resident_help" />
	<button label="增加..." name="add_banned_avatar_btn" />
	<button label="移除..." name="remove_banned_avatar_btn" />
	<button label="向地產發送消息..." name="message_estate_btn" />
	<button label="從地產將用戶踢走..." name="kick_user_from_estate_btn" />
</panel>
