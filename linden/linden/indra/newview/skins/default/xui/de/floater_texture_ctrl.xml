      </text>
      <button label="Hinzufügen" name="Add" width="70" />
      <button label="Entfernen" name="Remove" />
      <button label="Eigenschaften..." name="Browser" />
      <scroll_list name="local_name_list" >
	      <column name="unit_name" label="Name" />
	      <column name="unit_id_HIDDEN" label="ID" />
